{
    "name": "@vonage/ml-transformers",
    "version": "4.3.4",
    "author": "Guy Mininberg <guy.mininberg@vonage.com>",
    "contributors": [
        "Guy Mininberg <guy.mininberg@vonage.com>",
        "Jose Olivera <joseantonio.oliveraortega@vonage.com>",
        "Remi Deltombe <remi.deltombe@vonage.com>"
    ],
    "keywords": [
        "vonage",
        "vonage api",
        "client",
        "media processing",
        "insertable streams",
        "transformers",
        "machine learning",
        "background blur",
        "virtual background",
        "video background",
        "face masking",
        "opentok",
        "nexmo"
    ],
    "scripts": {
        "build": "npm-run-all build:js build:static",
        "build:js": "tsc && vite build && tsc --declaration --emitDeclarationOnly --outDir dist/types",
        "build:static": "node ./scripts/build-static.js && minify ./dist/ml-transformers.static.js > ./dist/ml-transformers.min.js ",
        "dev": "vite",
        "preview": "vite preview",
        "e2e:app": "vite ./tests/e2e/apps/vite",
        "e2e:app:run:vite": "vite ./tests/e2e/apps/vite",
        "e2e:app:run:webpack": "vite ./tests/e2e/apps/webpack/app",
        "e2e:app:install": "node ./tests/e2e/scripts/install-app.js && node ./tests/e2e/scripts/build-app.js",
        "test": "vitest --run",
        "test:install": "npm install && npm run build && npm pack && npm run e2e:app:install",
        "version:local": "node -pe 'require(\"./package.json\").version'",
        "version:published": "npm show @vonage/ml-transformers version"
    },
    "dependencies": {
        "@mediapipe/face_detection": "0.4.1646425229",
        "@mediapipe/face_mesh": "0.4.1633559619",
        "@mediapipe/hands": "0.4.1646424915",
        "@mediapipe/holistic": "0.5.1635989137",
        "@mediapipe/objectron": "0.4.1636596145",
        "@mediapipe/pose": "0.5.1635988162",
        "@mediapipe/selfie_segmentation": "0.1.1632777926",
        "@types/dom-mediacapture-transform": "^0.1.2",
        "@types/emscripten": "^1.39.6",
        "@types/node": "^17.0.10",
        "@types/offscreencanvas": "^2019.6.4",
        "@vonage/js-workerizer": "^1.0.1",
        "@vonage/media-processor": "2.0.0",
        "axios": "^0.25.0",
        "twgl.js": "^5.1.0",
        "typescript-optional": "3.0.0-alpha.3",
        "uuid": "^8.3.2",
        "wasm-feature-detect": "^1.2.11"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/main.d.ts",
    "main": "./dist/ml-transformers.umd.js",
    "module": "./dist/ml-transformers.es.js",
    "exports": {
        ".": {
            "import": "./dist/ml-transformers.es.js",
            "require": "./dist/ml-transformers.umd.js"
        }
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.11",
        "@types/chai": "^4.3.3",
        "@types/mocha": "^10.0.0",
        "@types/puppeteer": "^5.4.7",
        "@types/rimraf": "^3.0.2",
        "@vitest/ui": "^0.24.3",
        "chai": "^4.3.6",
        "chai-image": "file:./lib/libs/chai-image-3.0.0.tgz",
        "minify": "^9.1.0",
        "mocha": "^10.1.0",
        "node-fetch": "^3.3.0",
        "npm-run-all": "^4.1.5",
        "puppeteer": "^19.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.8.4",
        "vite": "3.1.8",
        "vitest": "^0.24.3"
    },
    "repository": "https://github.com/Vonage/vonage-media-transformers-samples",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    }
}
